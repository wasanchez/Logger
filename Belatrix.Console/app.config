    </connectionStrings>
    <appSettings>
        <add key="LogFileDirectory" value="."/> 
        
        <!--Log level type could be 
            Info: Log only information
            Warning: Log only warnings
            Error: Log only errors
            All: Log all type of logs 
            Or combine two of three types of logs
        -->
